{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "A web app showing the states of all (feature) branches and links to running docker containers for each feature.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "docker",
    "github"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "eslint . -c .eslintrc-with-pretty.js",
    "convert-readme": "asciidoctorjs -s -v README.adoc",
    "dev": "npm run convert-readme ; webpack-dev-server -w -d  --progress  --open",
    "test": "NODE_ENV=test istanbul cover -v -i '**/*.js'  _mocha test/** -- --compilers js:babel-core/register",
    "test:circle": "XUNIT_FILE=$CIRCLE_TEST_REPORTS/mocha-xunit.xml npm run test -- -R xunit-file",
    "prestart": "npm run convert-readme ; webpack --profile --progress",
    "start": "node ."
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": "https://github.com/lowsky/dashboard",
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "express": "^4.14.1",
    "helmet": "3.6.1",
    "whatwg-fetch": "^2.0.0"
  },
  "devDependencies": {
    "asciidoctor-cli": "^1.5.5-beta.3",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-helper-builder-react-jsx": "^6.8.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-relay-plugin": "0.10.0",
    "chai": "^4.0.0",
    "enzyme": "2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-graphql": "^0.8.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-react": "^7.0.1",
    "express-graphql": "^0.6.3",
    "file-loader": "^0.11.0",
    "graphql": "^0.9.1",
    "graphql-relay": "^0.5.1",
    "graphql-schema-from-introspection": "0.1.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.4",
    "jsdom": "^10.1.0",
    "jsx-loader": "^0.13.2",
    "minimatch": "^3.0.3",
    "mocha": "^3.2.0",
    "node-libs-browser": "^2.0.0",
    "nodemon": "^1.10.0",
    "prettier": "^1.2.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.7",
    "react-relay": "^0.10.0",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5",
    "xunit-file": "^1.0.0"
  }
}
