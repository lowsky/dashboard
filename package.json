{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "A web app showing the states of all (feature) branches and links to running docker containers for each feature.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "docker",
    "github"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "relay": "relay-compiler --src ./src --schema ./schema.graphql",
    "build": "npm run convert-readme ; webpack --profile --progress",
    "lint": "eslint .",
    "convert-readme": "remark README.md --use remark-html > public/README.html; mkdir -p public/assets/images/ ; cp -rv assets/images/DashboardDemo.png public/assets/images/",
    "dev": "npm run convert-readme ; webpack-dev-server -w -d  --progress  --open --hot",
    "test": "NODE_ENV=test istanbul cover -v -i '**/*.js' _mocha test/**/*.js",
    "test:circleci": "XUNIT_FILE=$CIRCLE_TEST_REPORTS/mocha-xunit.xml npm run test -- -R xunit-file",
    "start": "babel-node ."
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": "https://github.com/lowsky/dashboard",
  "dependencies": {
    "babel-runtime": "6.26.0",
    "cors": "2.8.4",
    "dotenv": "5.0.0",
    "express": "4.16.2",
    "github-api": "https://github.com/lowsky/github-api/archive/supportInUsedInBrowser.tar.gz",
    "helmet": "3.11.0",
    "relay-runtime": "1.4.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-helper-builder-react-jsx": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-relay": "1.4.1",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.18.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-graphql": "1.5.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.6.1",
    "express-graphql": "0.6.11",
    "file-loader": "1.1.6",
    "graphql": "0.12.3",
    "graphql-relay": "0.5.3",
    "graphql-schema-from-introspection": "0.1.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "2.30.1",
    "istanbul": "0.4.5",
    "jsx-loader": "0.13.2",
    "minimatch": "3.0.4",
    "mocha": "5.0.1",
    "node-libs-browser": "2.1.0",
    "prettier": "1.10.2",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "4.0.0-beta.14",
    "react-relay": "1.4.1",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-test-renderer": "16.2.0",
    "relay-compiler": "1.4.1",
    "remark": "9.0.0",
    "remark-cli": "5.0.0",
    "remark-html": "7.0.0",
    "remark-preset-lint-markdown-style-guide": "2.1.1",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1",
    "xunit-file": "1.0.0"
  }
}
