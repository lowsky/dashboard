{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "A web app showing the states of all (feature) branches and links to running docker containers for each feature.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "docker",
    "github"
  ],
  "engines": {
    "node": ">=8"
  },
  "proxy": "http://localhost:3000",
  "scripts": {
    "cra-start": "PORT=8080 react-app-rewired start",
    "cra-build": "react-app-rewired build",
    "cra-test": "react-app-rewired test --env=jsdom",
    "relay": "relay-compiler --src ./src --schema ./src/relay/data/schema.graphql",
    "build": "npm run relay ; npm run cra-build",
    "lint": "eslint .",
    "dev": "yarn run cra-start",
    "test": "NODE_ENV=test istanbul cover -v -i '**/*.js' _mocha test/**/*.js",
    "test:circleci": "XUNIT_FILE=$CIRCLE_TEST_REPORTS/mocha-xunit.xml npm run test -- -R xunit-file",
    "start": "babel-node .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": "https://github.com/lowsky/dashboard",
  "dependencies": {
    "babel-runtime": "6.26.0",
    "cors": "2.8.4",
    "dotenv": "5.0.0",
    "express": "4.16.2",
    "github-api": "https://github.com/lowsky/github-api/archive/supportInUsedInBrowser.tar.gz",
    "helmet": "3.11.0",
    "react-scripts": "1.1.1",
    "relay-runtime": "1.4.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-helper-builder-react-jsx": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-relay": "1.4.1",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.18.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-graphql": "1.5.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.6.1",
    "express-graphql": "0.6.12",
    "graphql": "0.13.1",
    "graphql-relay": "0.5.4",
    "graphql-schema-from-introspection": "0.1.2",
    "istanbul": "0.4.5",
    "mocha": "5.0.1",
    "prettier": "1.10.2",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-app-rewired": "1.4.1",
    "react-dom": "16.2.0",
    "react-hot-loader": "4.0.0-rc.0",
    "react-relay": "1.4.1",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-test-renderer": "16.2.0",
    "relay-compiler": "1.4.1",
    "xunit-file": "1.0.0",
    "@storybook/react": "3.3.12",
    "@storybook/addon-actions": "3.3.12",
    "@storybook/addon-links": "3.3.12",
    "@storybook/addons": "3.3.12"
  }
}
