{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "A web app showing the states of all (feature) branches and links to running docker containers for each feature.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "docker",
    "github"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "relay": "relay-compiler --src ./src --schema ./schema.graphql",
    "build": "npm run convert-readme ; webpack --profile --progress",
    "lint": "eslint .",
    "convert-readme": " remark README.md --use remark-html > README.html",
    "dev": "npm run convert-readme ; webpack-dev-server -w -d  --progress  --open --hot",
    "test": "NODE_ENV=test istanbul cover -v -i '**/*.js' _mocha test/**/*.js",
    "test:circleci": "XUNIT_FILE=$CIRCLE_TEST_REPORTS/mocha-xunit.xml npm run test -- -R xunit-file",
    "start": "babel-node ."
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": "https://github.com/lowsky/dashboard",
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "github-api": "https://github.com/lowsky/github-api/archive/supportInUsedInBrowser.tar.gz",
    "helmet": "3.9.0",
    "relay-runtime": "^1.1.0",
    "remark": "8.0.0",
    "remark-cli": "4.0.0",
    "remark-html": "6.0.1",
    "whatwg-fetch": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.13.2",
    "babel-eslint": "^8.0.1",
    "babel-helper-builder-react-jsx": "^6.8.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-relay": "^1.1.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.1",
    "chai": "^4.0.1",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.5.0",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-graphql": "1.4.0-1",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-react": "^7.2.0",
    "express-graphql": "^0.6.6",
    "file-loader": "^1.0.0",
    "graphql": "^0.11.1",
    "graphql-relay": "^0.5.2",
    "graphql-schema-from-introspection": "0.1.2",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.4",
    "jsdom": "^11.1.0",
    "jsx-loader": "^0.13.2",
    "minimatch": "^3.0.3",
    "mocha": "^4.0.0",
    "node-libs-browser": "^2.0.0",
    "nodemon": "^1.10.0",
    "prettier": "^1.5.3",
    "prop-types": "15.6.0",
    "react": "^16.0.0",
    "react-dom": "16.0.0",
    "react-hot-loader": "3.1.1",
    "react-relay": "^1.1.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-test-renderer": "^16.0.0",
    "relay-compiler": "1.4.1",
    "remark-cli": "^4.0.0",
    "remark-html": "^6.0.1",
    "remark-preset-lint-markdown-style-guide": "^2.1.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.6.1",
    "xunit-file": "^1.0.0"
  }
}
