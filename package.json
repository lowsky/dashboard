{
  "name": "dashboard",
  "version": "2.0.0",
  "description": "A web site showing the build states of all (feature) branches on one page.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "github"
  ],
  "engines": {
    "node": ">=12.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build:relay": "relay-compiler --src . --schema schema/schema.graphql --language typescript --exclude '**/.next/**' '**/node_modules/**' '**/test/**'  '**/__generated__/**' '**/schema/**' --include '**/container/**' '**/components/**' '**/pages/**' '**/relay/**' '**/queries/**' ",
    "vercel-build": " yarn run vercel-build:without-chromatic ; yarn run chromatic-test",
    "vercel-build:without-chromatic": "yarn run build:relay ; yarn run build:storybook; yarn run build",
    "_lint": "eslint .",
    "test": "jest",
    "coverage": "npm run test -- --coverage --coveragePathIgnorePatterns=src/relay/__generated__",
    "test:ci": "mkdir -p test-reports ; yarn run test:circleci",
    "test:vis": "start-server-and-test 'yarn run storybook -- --quiet --ci' 9009 'cypress run --record'",
    "test:storybook": "cypress run --record",
    "test:circleci": "TEST_REPORT_FILENAME=test-reports/jest-junit.xml npm test -- --ci --testResultsProcessor=./node_modules/jest-junit-reporter --all --watch=false --watchAll=false --forceExit",
    "chromatic-test": "chromatic --exit-zero-on-changes -d public/story-book --ci --allow-console-errors --exit-once-uploaded '*'",
    "storybook": "start-storybook -p 9009",
    "build:storybook": "build-storybook -o public/story-book --quiet"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": ".",
  "dependencies": {
    "apollo-server-micro": "2.21.0",
    "bulma": "0.9.2",
    "github-api": "lowsky/github-api#supportInUsedInBrowser",
    "graphql": "15.5.0",
    "next": "10.0.7",
    "next-images": "1.7.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-config": "5.1.1",
    "whatwg-fetch": "3.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-transform-typescript": "7.12.1",
    "@fortawesome/fontawesome-svg-core": "1.2.34",
    "@fortawesome/free-brands-svg-icons": "5.15.2",
    "@fortawesome/free-solid-svg-icons": "5.15.2",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@graphql-tools/schema": "7.1.3",
    "@storybook/addon-actions": "6.1.18",
    "@storybook/addon-essentials": "6.1.18",
    "@storybook/addon-links": "6.1.18",
    "@storybook/addon-storyshots": "6.1.18",
    "@storybook/react": "6.1.18",
    "@types/react": "17.0.2",
    "babel-loader": "8.2.2",
    "babel-plugin-relay": "10.1.3",
    "chromatic": "5.6.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "graphql": "15.5.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "react-relay": "10.1.3",
    "react-router-dom": "5.2.0",
    "relay-compiler": "10.1.3",
    "relay-compiler-language-typescript": "13.0.6",
    "ts-jest": "26.5.1",
    "typescript": "4.1.5"
  }
}
