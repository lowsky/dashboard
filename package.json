{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "A web app showing the states of all (feature) branches and links to running docker containers for each feature.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "github"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "postinstall": "cd server ; yarn",
    "dev": "npm-run-all --parallel start:server start",
    "start": "react-scripts start",
    "start:server": "cd server ; npm run start",
    "cra-build": "react-scripts build",
    "relay": "relay-compiler --src ./src --schema ./src/relay/data/schema.graphql",
    "build": "npm run relay ; npm run build:server ; npm run cra-build ; npm run build-storybook -- -o build/story-book",
    "build:server": "cd server ; npm run build",
    "lint": "eslint .",
    "test": "react-scripts test --no-cache ",
    "coverage": "npm run test -- --coverage --coveragePathIgnorePatterns=src/relay/__generated__",
    "test:ci": "mkdir -p test-reports ; npm run test:circleci",
    "test:circleci": "TEST_REPORT_FILENAME=test-reports/jest-junit.xml npm test -- --ci --testResultsProcessor=./node_modules/jest-junit-reporter --all --watch=false --watchAll=false --forceExit",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": ".",
  "dependencies": {
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "bulma": "0.7.2",
    "codacy-coverage": "3.4.0",
    "github-api": "lowsky/github-api#supportInUsedInBrowser",
    "react-scripts": "2.1.3",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "4.1.11",
    "@storybook/addon-actions": "4.1.11",
    "@storybook/addon-links": "4.1.11",
    "@storybook/addon-storyshots": "4.1.11",
    "@storybook/addon-storysource": "4.1.11",
    "@storybook/addon-viewport": "4.1.11",
    "@storybook/addons": "4.1.11",
    "@storybook/react": "4.1.11",
    "babel-plugin-relay": "2.0.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.8.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-graphql": "3.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "graphql": "14.1.1",
    "graphql-relay": "0.6.0",
    "http-proxy-middleware": "0.19.1",
    "jest-junit-reporter": "1.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.3",
    "prop-types": "15.6.2",
    "react": "16.8.0-alpha.1",
    "react-dom": "16.8.0-alpha.1",
    "react-relay": "2.0.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.8.0-alpha.1",
    "relay-compiler": "2.0.0",
    "storybook-addon-jsx": "6.0.0",
    "xunit-file": "1.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
