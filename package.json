{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "A web app showing the states of all (feature) branches and links to running docker containers for each feature.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "github"
  ],
  "engines": {
    "node": ">=12.1"
  },
  "scripts": {
    "postinstall": "cd server ; yarn",
    "dev": "npm-run-all --parallel start:server start",
    "start": "react-scripts start",
    "start:server": "cd server ; npm run start",
    "cra-build": "react-scripts build",
    "relay": "relay-compiler --src ./src --schema ./src/relay/data/schema.graphql",
    "build": "npm run relay ; npm run build:server && npm run cra-build && npm run build-storybook -- -o build/story-book --quiet && npm run chromatic-test",
    "build:server": "cd server ; npm run build",
    "lint": "eslint .",
    "test": "react-scripts test --no-cache ",
    "coverage": "npm run test -- --coverage --coveragePathIgnorePatterns=src/relay/__generated__",
    "test:ci": "mkdir -p test-reports ; npm run test:circleci",
    "test:vis": "start-server-and-test 'npm run storybook -- --quiet --ci' 9009 'cypress run --record'",
    "test:storybook": "cypress run --record",
    "test:circleci": "TEST_REPORT_FILENAME=test-reports/jest-junit.xml npm test -- --ci --testResultsProcessor=./node_modules/jest-junit-reporter --all --watch=false --watchAll=false --forceExit",
    "chromatic": "chromatic test  --do-not-start",
    "chromatic-test": "chromatic test  --do-not-start --exit-zero-on-changes -d build/story-book --debug ",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": ".",
  "dependencies": {
    "@babel/core": "7.10.5",
    "@babel/node": "7.10.5",
    "bulma": "0.9.0",
    "chromatic": "5.0.0",
    "github-api": "lowsky/github-api#supportInUsedInBrowser",
    "react-router-config": "5.1.1",
    "react-scripts": "3.4.1",
    "relay-runtime": "10.0.1",
    "whatwg-fetch": "3.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx-source": "7.10.5",
    "@emotion/core": "10.0.28",
    "@storybook/addon-docs": "5.3.19",
    "@storybook/addon-storyshots": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/preset-create-react-app": "3.1.4",
    "@storybook/react": "5.3.19",
    "@types/jest": "26.0.5",
    "@types/node": "12.12.53",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/relay-runtime": "10.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-relay": "10.0.1",
    "cypress-match-screenshot": "1.1.0",
    "emotion-theming": "10.0.27",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.5",
    "graphql": "15.3.0",
    "http-proxy-middleware": "1.0.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-relay": "10.0.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "16.13.1",
    "relay-compiler": "10.0.0",
    "relay-compiler-language-typescript": "13.0.0",
    "relay-config": "10.0.0",
    "start-server-and-test": "1.11.2",
    "storybook-addon-deps": "2.0.1",
    "storybook-chromatic": "4.0.2",
    "typescript": "3.9.7"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
