{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "A web app showing the states of all (feature) branches and links to running docker containers for each feature.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "github"
  ],
  "engines": {
    "node": ">=12.1"
  },
  "scripts": {
    "postinstall": "cd server ; yarn",
    "dev": "npm-run-all --parallel start:server start",
    "start": "react-scripts start",
    "start:server": "cd server ; yarn run start",
    "build:cra": "react-scripts build",
    "build:relay": "relay-compiler --src ./src --schema ./src/relay/data/schema.graphql",
    "build": "yarn run build:relay ; npm-run-all --parallel build:server build:storybook build:cra ; yarn run chromatic-test",
    "build-now": "yarn run build:relay ; npm-run-all --parallel build:storybook build:server build:cra ; mkdir -p build/api ; cp server/lambda/graphql.js build/api/graphql.js  ; npm-run-all --parallel  chromatic-test",
    "build:server": "cd server ; yarn run build",
    "lint": "eslint .",
    "test": "react-scripts test --no-cache ",
    "coverage": "npm run test -- --coverage --coveragePathIgnorePatterns=src/relay/__generated__",
    "test:ci": "mkdir -p test-reports ; yarn run test:circleci",
    "test:vis": "start-server-and-test 'yarn run storybook -- --quiet --ci' 9009 'cypress run --record'",
    "test:storybook": "cypress run --record",
    "test:circleci": "TEST_REPORT_FILENAME=test-reports/jest-junit.xml npm test -- --ci --testResultsProcessor=./node_modules/jest-junit-reporter --all --watch=false --watchAll=false --forceExit",
    "chromatic-test": "chromatic  --exit-zero-on-changes -d build/story-book --ci",
    "storybook": "start-storybook -p 9009",
    "build:storybook": "build-storybook -o build/story-book --quiet --no-dll"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": ".",
  "dependencies": {
    "@babel/core": "7.11.6",
    "@babel/node": "7.10.5",
    "bulma": "0.9.0",
    "github-api": "lowsky/github-api#supportInUsedInBrowser",
    "react-router-config": "5.1.1",
    "react-scripts": "3.4.3",
    "whatwg-fetch": "3.4.1"
  },
  "devDependencies": {
    "@babel/helper-create-regexp-features-plugin": "7.10.4",
    "@babel/helper-hoist-variables": "7.10.4",
    "@babel/helper-skip-transparent-expression-wrappers": "7.11.0",
    "@babel/plugin-transform-react-jsx-source": "7.10.5",
    "@storybook/addon-docs": "6.0.21",
    "@storybook/addon-storyshots": "6.0.21",
    "@storybook/addons": "6.0.21",
    "@storybook/components": "6.0.21",
    "@storybook/preset-create-react-app": "3.1.4",
    "@storybook/react": "6.0.21",
    "@types/jest": "26.0.14",
    "@types/node": "14.14.2",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.6",
    "@types/relay-runtime": "10.0.3",
    "babel-loader": "8.1.0",
    "babel-plugin-relay": "10.0.1",
    "chromatic": "5.2.0",
    "cypress-match-screenshot": "1.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.4",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "graphql": "15.3.0",
    "http-proxy-middleware": "1.0.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-is": "17.0.0",
    "react-relay": "10.0.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "16.14.0",
    "relay-compiler": "10.0.1",
    "relay-compiler-language-typescript": "13.0.1",
    "relay-config": "10.0.1",
    "start-server-and-test": "1.11.3",
    "storybook-addon-deps": "2.1.3",
    "typescript": "4.0.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
