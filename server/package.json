{
  "name": "dashboard-github-server",
  "version": "1.1.0",
  "description": "A graphql proxy for some github rest api, using netlify lambda function",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "express",
    "github",
    "graphql"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "dev": "node -r dotenv/config node_modules/.bin/netlify-lambda serve src/lambda",
    "build": "netlify-lambda build src/lambda",
    "start": "babel-node .",
    "dev:server": "PORT=4000 nodemon -e js,graphql -x babel-node ."
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "dependencies": {
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "apollo-server-lambda": "2.3.2",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-graphql": "0.7.1",
    "github-api": "lowsky/github-api#supportInUsedInBrowser",
    "graphql": "14.1.1",
    "graphql-yoga": "1.17.1",
    "helmet": "3.15.0",
    "netlify-lambda": "1.3.0",
    "utf-8-validate": "5.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "nodemon": "1.18.9"
  }
}
