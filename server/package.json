{
  "name": "dashboard-github-server",
  "version": "1.0.0",
  "description": "A graphql proxy for some github rest api.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "express",
    "github",
    "graphql"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start:lambda": "node -r dotenv/config node_modules/.bin/netlify-lambda serve src/lambda",
    "build:lambda": "netlify-lambda build src/lambda",
    "lint": "eslint .",
    "start": "babel-node .",
    "dev": "PORT=4000 nodemon -e js,graphql -x babel-node ."
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "dependencies": {
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-graphql": "0.7.1",
    "github-api": "lowsky/github-api#supportInUsedInBrowser",
    "graphql": "14.1.1",
    "graphql-yoga": "1.16.9",
    "helmet": "3.15.0",
    "netlify-lambda": "1.2.0",
    "utf-8-validate": "5.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "babel-eslint": "10.0.1",
    "eslint": "5.9.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-graphql": "2.1.1",
    "eslint-plugin-prettier": "3.0.0",
    "nodemon": "1.18.9",
    "prettier": "1.15.1"
  }
}
